<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN" "http://www.opensymphony.com/xwork/xwork-1.1.dtd">

<xwork>
    <include file="webwork-default.xml"/>

    <package name="default" extends="webwork-default">
        <action name="test">
            <result type="freemarker">test.ftl</result>
        </action>

        <action name="person" class="com.acme.GetPerson">
            <result type="freemarker">/person.ftl</result>
        </action>

        <action name="newPerson" class="com.acme.CreatePerson">
            <result type="redirect">/person.action?personId=${id}</result>
        </action>

        <action name="tableTest" class="com.opensymphony.webwork.sandbox.TableTest">
            <result type="freemarker">tableTest.ftl</result>
        </action>
    </package>
</xwork>
